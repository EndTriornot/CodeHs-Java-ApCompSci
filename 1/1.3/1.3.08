public class FallingBodies
{
    public static void main(String[] args)
    {
        final double g = 9.8;
        double t = 23.0;
        double h;
        double v;
        
        h = 0.5 * g * t*t;
        v = g * t;
        
        System.out.println("The height is" + h);
        System.out.println("The velocity is" + v);
    }
}
